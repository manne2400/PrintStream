{
  "name": "printstream",
  "productName": "PrintStream",
  "version": "0.3.1",
  "description": "3D Print Management System",
  "main": "build/electron.js",
  "scripts": {
    "start": "webpack --mode development && electron-rebuild -f && electron .",
    "dev": "webpack --mode development --watch & electron .",
    "build": "webpack --mode production && electron-builder",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dist:linux": "electron-builder --linux",
    "setup-python": "node scripts/setup-python.js",
    "postinstall": "npm run setup-python"
  },
  "keywords": [],
  "author": {
    "name": "Jacob Manscher",
    "email": "jacobm@printstream.app"
  },
  "license": "MIT",
  "dependencies": {
    "@chakra-ui/react": "^2.10.3",
    "@electron/remote": "^2.1.2",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@heroicons/react": "^2.0.18",
    "@types/pdfkit": "^0.13.5",
    "chart.js": "^4.4.0",
    "electron-is-dev": "^2.0.0",
    "framer-motion": "^10.18.0",
    "pdfkit": "^0.15.1",
    "printstream": "file:",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@types/sqlite3": "^3.1.11",
    "adm-zip": "^0.5.16",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "download": "^8.0.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "electron-rebuild": "^3.2.9",
    "html-webpack-plugin": "^5.5.3",
    "node-loader": "^2.0.0",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.1",
    "typescript": "~4.9.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "build": {
    "appId": "com.printstream.app",
    "productName": "PrintStream",
    "win": {
      "target": "nsis",
      "icon": "src/assets/icon.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Utility",
      "icon": "src/assets/icon.png"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "py_tools/**/*",
      "LICENSE.md"
    ],
    "extraResources": [
      {
        "from": "py_tools/python",
        "to": "py_tools/python",
        "filter": [
          "**/*"
        ]
      }
    ],
    "asarUnpack": [
      "py_tools/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "dist"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "license": "LICENSE.md",
      "installerIcon": "src/assets/icon.ico",
      "uninstallerIcon": "src/assets/icon.ico",
      "installerHeaderIcon": "src/assets/icon.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "PrintStream",
      "perMachine": false,
      "runAfterFinish": true
    }
  }
}
